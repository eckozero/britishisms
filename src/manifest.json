{
  "manifest_version": 2,
  "name": "Britishisms",
  "version": "0.2",
  "description": "Changes American English to British English ",
  "browser_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    },
    "default_title": "Britishisms"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": [
        "britishism.js",
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons":
  {
    "128": "icons/128.png"
  }
}

